{
	"ecmaFeatures": {
	  "jsx": true,
	  "modules": true
	},
	"plugins": [
	  "react"
	],
	"rules": {
	  "react/jsx-uses-react": 2,
	  "react/jsx-uses-vars": 2,
	  "react/react-in-jsx-scope": 2,
	  "react/prop-types": 0,
	  "arrow-parens": 0
	},
	"extends": ["standard", "standard-react"],
	"parser": "babel-eslint"
  }
